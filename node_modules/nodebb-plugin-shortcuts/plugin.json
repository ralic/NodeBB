{
  "id": "nodebb-plugin-shortcuts",
  "version": "1.1.2+936",
  "name": "Shortcuts",
  "description": "Adds advanced keyboard support to your NodeBB.",
  "library": "lib/main.js",
  "less": ["public/styles/main.less"],
  "scripts": ["public/scripts/"],
  "staticDirs": {"static": "public/static/"},
  "languages": "public/locales",
  "defaultLang": "en_GB",
  "templates": "public/templates",
  "hooks": [
    {"hook": "static:app.load", "method": "init"},
    {"hook": "filter:admin.header.build", "method": "adminMenu"}
  ]
}
