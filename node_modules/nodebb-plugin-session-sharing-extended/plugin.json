{
	"id": "nodebb-plugin-session-sharing-extended",
	"url": "https://github.com/alecananian/nodebb-plugin-session-sharing-extended",
	"library": "./library.js",
	"hooks": [
		{ "hook": "static:app.preload", "method": "addMiddleware" },
		{ "hook": "static:app.load", "method": "init" },
		{ "hook": "filter:admin.header.build", "method": "addAdminNavigation" },
		{ "hook": "static:user.loggedOut", "method": "cleanup" }
	],
	"less": [
		"static/style.less"
	],
	"scripts": [
		"static/lib/main.js"
	],
	"acpScripts": [
		"static/lib/admin.js"
	],
	"templates": "static/templates"
}
